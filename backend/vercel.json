{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.6.0"
        }
    },
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/public/build/$1"
        },
        {
            "src": "/presentation/(.*)",
            "dest": "/public/presentation/$1"
        },
        {
            "src": "/presentacion/(.*)",
            "dest": "/public/presentacion/$1"
        },
        {
            "src": "/storage/(.*)",
            "dest": "/public/storage/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https://${VERCEL_URL}",
        "APP_KEY": "base64:JbdAKjZ7PcXYwe044RYgSbrv+WQizhXz1z6M3gmGlLM=",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",
        "SESSION_DRIVER": "cookie",
        "LOG_CHANNEL": "stderr",
        "DB_CONNECTION": "mysql",
        "DB_HOST": "nozomi.proxy.rlwy.net",
        "DB_PORT": "43356",
        "DB_DATABASE": "railway",
        "DB_USERNAME": "root",
        "DB_PASSWORD": "SXxjnZQiZscSAHAEFsrjdSViwKHlSJQH"
    }
