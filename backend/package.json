{
    "$schema": "https://www.schemastore.org/package.json",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "dev": "vite",
        "start": "concurrently \"vite\" \"php artisan serve\"",
        "prod": "touch database/database.sqlite && php artisan migrate --force --seed && concurrently \"php artisan serve --host=0.0.0.0 --port=$PORT\" \"php artisan reverb:start --host=0.0.0.0 --port=8080\""
    },
    "devDependencies": {
        "@tailwindcss/vite": "^4.0.0",
        "@vitejs/plugin-react": "^5.1.1",
        "axios": "^1.11.0",
        "laravel-vite-plugin": "^2.0.0",
        "tailwindcss": "^4.0.0",
        "vite": "^7.0.7",
        "vite-plugin-pwa": "^1.2.0"
    },
    "dependencies": {
        "concurrently": "^9.0.1",
        "i18next": "^25.7.3",
        "i18next-browser-languagedetector": "^8.2.0",
        "laravel-echo": "^2.2.6",
        "leaflet": "^1.9.4",
        "lucide-react": "^0.562.0",
        "pusher-js": "^8.4.0",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "react-helmet-async": "^2.0.5",
        "react-i18next": "^16.5.0",
        "react-is": "^19.2.3",
        "react-leaflet": "^5.0.0",
        "react-router-dom": "^7.9.6",
        "recharts": "^3.5.1",
        "sonner": "^2.0.7"
    },
    "overrides": {
        "react-helmet-async": {
            "react": "^19.0.0",
            "react-dom": "^19.0.0"
        }
    }
}